

	<%= render "index_header" %>











	<section class="pob_sech">
		<div class="container">
			<div class="row">
			<div class="col-md-8 col-sm-8 col-xs-12">
			<h3 class="po_hp">Поиск по публикациям</h3>
			<form class="poisk_h_ing ff_pabb ">      
